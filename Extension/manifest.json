
{
  "name": "SiteScraperConfig",
  "description": "Generates A Config File For SiteScraper Python Script",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "Service/background.js"
  },
  "permissions": [
      "sidePanel"
  ],
  "side_panel" : {
    "default_path" : "View/ui.html"
  },
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["Content/listen.js"]
  }
]

}
