{
  "name": "ElementFinder",
  "description": "Generates A Config File For SiteScraper Python Script",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": "icon.png",
    "default_title": "ElementFinder"
  },
  "background": { "service_worker": "Service/background.js" },
  "permissions": [
    "scripting",
    "tabs",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
